snippet console-log
alias cl
options head
  console.log(${1:TARGET});
  ${0}

snippet console-error
alias ce
options head
  console.error(${0:TARGET});


snippet try-catch
alias try
  try {
    ${1: code}
  } catch(e) {
    ${2: error}
  }

snippet styled-component
alias sc
options head
  export const ${1: name} = styled.${2: type}\`
    ${0}
  \`;
