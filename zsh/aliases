# npm
alias n="node"
alias ni="npm install"
alias nis="npm install --save"
alias nisd="npm install --save-dev"
alias nig="npm install -g"
alias nl="npm link"
alias nr="npm run"
alias nt="npm test"
alias ns="npm start"


# tmux
alias tl="tmux ls"
alias ta="tmux attach -t"
alias tk="tmux kill-session -t"
alias td="tmux detach"
alias tnew="echo use tat!!"
alias tat="new-tmux-from-dir-name"


# docker-compose
alias docker-clean='docker rmi -f $(docker images -f "dangling=true" -q) && docker volume prune -f'
alias docker-clean-test='docker rmi $(docker images --filter=reference="test*:latest" -q) -f'
